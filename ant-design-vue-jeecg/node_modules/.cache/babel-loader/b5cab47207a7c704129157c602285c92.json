ctedRowKeys[i] == id) {\n                _this2.selectedRowKeys.splice(i, 1);\n\n                break;\n              }\n            }\n          }\n\n          that.loadData();\n        } else {\n          that.$message.warning(res.message);\n        }\n      });\n    },\n    open: function open(record) {\n      //console.log(record);\n      this.currentDeptId = record.id;\n      this.loadData(1);\n    },\n    clearList: function clearList() {\n      this.currentDeptId = '';\n      this.dataSource = [];\n    },\n    hasSelectDept: function hasSelectDept() {\n      if (this.currentDeptId == '') {\n        this.$message.error(\"请选择一个部门!\");\n        return false;\n      }\n\n      return true;\n    },\n    handleAddUserDepart: function handleAddUserDepart() {\n      if (this.currentDeptId == '') {\n        this.$message.error(\"请选择一个部门!\");\n      } else {\n        this.$refs.selectUserModal.visible = true;\n      }\n    },\n    handleEdit: function handleEdit(record) {\n      this.$refs.modalForm.title = \"编辑\";\n      this.$refs.modalForm.departDisabled = true;\n      this.$refs.modalForm.disableSubmit = false;\n      this.$refs.modalForm.edit(record);\n    },\n    handleAdd: function handleAdd() {\n      if (this.currentDeptId == '') {\n        this.$message.error(\"请选择一个部门!\");\n      } else {\n        this.$refs.modalForm.departDisabled = true;\n        this.$refs.modalForm.userDepartModel.departIdList = [this.currentDeptId]; //传入一个部门id\n\n        this.$refs.modalForm.add();\n        this.$refs.modalForm.title = \"新增\";\n      }\n    },\n    selectOK: function selectOK(data) {\n      var _this3 = this;\n\n      var params = {};\n      params.depId = this.currentDeptId;\n      params.userIdList = [];\n\n      for (var a = 0; a < data.length; a++) {\n        params.userIdList.push(data[a]);\n      }\n\n      console.log(params);\n      postAction(this.url.edit, params).then(function (res) {\n        if (res.success) {\n          _this3.$message.success(res.messag